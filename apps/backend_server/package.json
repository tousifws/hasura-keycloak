{
    "name": "backend_server",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "dev": "dotenv -e .env.development.local -- nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "hasura": "export NODE_OPTIONS=\"\" && hasura --project ../hasura --envfile ../backend_server/.env.development.local --skip-update-check",
        "generate": "pnpm graphql-codegen",
        "kysely:gen": "dotenv -e .env.development.local -- kysely-codegen",
        "kysely:seed": "dotenv -e .env.development.local -- ts-node seeder/index.ts",
        "postinstall": "kysely:gen"
    },
    "dependencies": {
        "@fastify/cookie": "8.3.0",
        "@fastify/secure-session": "6.0.0",
        "@golevelup/nestjs-common": "^1.4.4",
        "@golevelup/nestjs-discovery": "^3.0.0",
        "@golevelup/nestjs-graphql-request": "0.1.14",
        "@golevelup/nestjs-hasura": "3.0.2",
        "@golevelup/nestjs-modules": "^0.6.1",
        "@nestjs/common": "9.3.8",
        "@nestjs/core": "9.3.8",
        "@nestjs/graphql": "10.2.0",
        "@nestjs/jwt": "10.0.2",
        "@nestjs/mapped-types": "*",
        "@nestjs/mercurius": "10.2.0",
        "@nestjs/platform-fastify": "9.4.0",
        "dotenv": "16.0.3",
        "envalid": "7.3.1",
        "graphql": "16.6.0",
        "graphql-request": "5.1.0",
        "ioredis": "5.2.5",
        "jwks-rsa": "^3.0.1",
        "kysely": "^0.24.2",
        "mercurius": "12.0.1",
        "openid-client": "^5.4.0",
        "pg": "^8.10.0",
        "reflect-metadata": "0.1.13",
        "rimraf": "4.1.2",
        "rxjs": "7.5.6",
        "zod": "3.20.6"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "3.0.0",
        "@graphql-codegen/introspection": "3.0.0",
        "@graphql-codegen/typescript": "3.0.0",
        "@graphql-codegen/typescript-graphql-request": "4.5.8",
        "@graphql-codegen/typescript-operations": "3.0.0",
        "@nestjs/cli": "9.2.0",
        "@nestjs/schematics": "9.0.4",
        "@nestjs/testing": "9.3.8",
        "@types/jest": "29.4.0",
        "@types/lodash": "4.14.191",
        "@types/node": "18.13.0",
        "@types/supertest": "2.0.12",
        "eslint": "8.37.0",
        "eslint-config-custom": "workspace:*",
        "fastify": "^4.13.0",
        "jest": "29.4.3",
        "kysely-codegen": "^0.10.0",
        "source-map-support": "0.5.21",
        "supertest": "6.3.3",
        "ts-jest": "29.0.5",
        "ts-loader": "9.4.2",
        "ts-node": "10.9.1",
        "tsconfig": "workspace:*",
        "tsconfig-paths": "4.1.2",
        "typescript": "^4.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
